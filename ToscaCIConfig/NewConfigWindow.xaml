<Window x:Class="ToscaCIConfig.NewConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToscaCIConfig"
        mc:Ignorable="d"
        Title="Create New Test Configuration" Height="150" Width="400">
    <Grid>
        <StackPanel>
            <Label>Input The name of the Test Configuration</Label>
            <TextBox Margin="10" VerticalAlignment="Top" Height="25"></TextBox>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Width="60" Margin="5">Cancel</Button>
                <Button Width="50" Margin="5">Ok</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
